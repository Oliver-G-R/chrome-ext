{
    "name": "Chrome Extension test",
    "description": "Extension created to test how it works",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "./images/icons/icon-16x16.png",
        "32": "./images/icons/icon-32x32.png",
        "48": "./images/icons/icon-48x48.png",
        "128": "./images/icons/icon-128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./popup/popup.html",
        "default_icons": {
            "16": "./images/icons/icon-16x16.png",
            "32": "./images/icons/icon-32x32.png",
            "48": "./images/icons/icon-48x48.png",
            "128": "./images/icons/icon-128x128.png"
        }
    },
    "options_page": "./popup/options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>",
        "*://*/*"
    ]

}